{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@app/*": ["src/app/*"]
    },
    "skipLibCheck": true
  },
  "files": [],
  "include": [],

  "exclude": [
    "dist",
    "node_modules",
    "tmp",
    "obj",
    "obj/**/*",
    "out-tsc"
  ]

}
